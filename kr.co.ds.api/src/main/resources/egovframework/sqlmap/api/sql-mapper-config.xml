<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<!-- mybatis -->
<configuration>
	
    <typeAliases>
		<typeAlias alias="egovMap" type="org.egovframe.rte.psl.dataaccess.util.EgovMap"/>
		<typeAlias alias="searchVO" type="egovframework.example.sample.service.SampleDefaultVO"/>
		<typeAlias alias="sampleVO" type="egovframework.example.sample.service.SampleVO"/>
		
		<!-- api defaultSearch -->
		<typeAlias alias="fSearchEntity" type="egovframework.api.entity.FrontApiDefaultEntity"/>
		
		<!-- api 반환 entity -->
		<typeAlias alias="boardEntity" type="egovframework.api.entity.BulletinBoard"/>	<!-- 게시판[공지사항] -->
		<typeAlias alias="scanEntity" type="egovframework.api.entity.ScanEntity"/>		<!-- 스캔 -->
		<typeAlias alias="traceEntity" type="egovframework.api.entity.TraceEntity"/>	<!-- 화물추적 -->
		<typeAlias alias="unsongEntity" type="egovframework.api.entity.UnsongEntity"/>	<!-- 운송장 -->
		
		<typeAlias alias="masterCodeEntity" type="egovframework.api.entity.MasterCodeEntity"/>	<!-- 마스터코드 -->
		<typeAlias alias="agencyCodeEntity" type="egovframework.api.entity.AgencyCodeEntity"/>	<!-- 영업소코드 -->
		<typeAlias alias="carCodeEntity" type="egovframework.api.entity.CarCodeEntity"/>		<!-- 차량코드 -->
		
    </typeAliases>
    
    <!-- 한글 인코딩 설정 -->
    <typeHandlers>
    	<typeHandler handler="egovframework.common.util.OracleUsMybatisTypeHandler"  />
	</typeHandlers>
	
</configuration>